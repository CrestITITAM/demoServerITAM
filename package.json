{
  "name": "ePromptoITAM",
  "version": "2.0.7",
  "description": "ITAM Desktop App for Server",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "deploy": "electron-builder build --win --publish always"
  },
  "build": {
    "appId": "itam_apiId_server",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "images/ePrompto.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrestITITAM/demoServerITAM.git"
  },
  "author": "CrestIT Consulting Pvt Ltd",
  "license": "ISC",
  "devDependencies": {
    "auto-launch": "^5.0.5",
    "child_process": "^1.0.2",
    "csvtojson": "^2.0.10",
    "electron": "^8.4.1",
    "electron-builder": "^22.8.0",
    "electron-log": "^4.2.2",
    "electron-packager": "^15.0.0",
    "electron-updater": "^4.3.8",
    "fs": "0.0.1-security",
    "ip": "^1.1.5",
    "jquery": "^3.5.1",
    "json2csv": "^5.0.1",
    "mv": "^2.1.1",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "node-disk-info": "^1.1.0",
    "node-machine-id": "^1.1.12",
    "node-os-utils": "^1.3.0",
    "ps-list": "^7.2.0",
    "request": "^2.88.2",
    "systeminformation": "^4.26.10"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "child_process": "^1.0.2",
    "csvtojson": "^2.0.10",
    "electron-log": "^4.2.2",
    "electron-packager": "^15.0.0",
    "electron-updater": "^4.3.8",
    "fs": "0.0.1-security",
    "ip": "^1.1.5",
    "jquery": "^3.5.1",
    "json2csv": "^5.0.1",
    "mv": "^2.1.1",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "node-disk-info": "^1.1.0",
    "node-machine-id": "^1.1.12",
    "node-os-utils": "^1.3.0",
    "ps-list": "^7.2.0",
    "request": "^2.88.2",
    "systeminformation": "^4.26.10"
  }
}
